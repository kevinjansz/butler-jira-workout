pipeline {
     
     agent any
     
     stages {
         
         stage('build') {
             steps {
                 echo 'Building...'
             }
         }
          
         stage('deploy to dev') {
             steps {
                 echo 'Deploying to DEV .....'
             }
         }

         stage('deploy to test') {
             steps {
                 echo 'Deploying...'
             }
         }
        
         stage('deploy to staging') {
             steps {
                 echo 'Deploying...'
             }
         }

         stage('deploy to prod-clone') {
             steps {
                 echo 'Deploying to PROD CLONE ...'
             }
         }

         stage('deploy to prod') {
             steps {
                 echo 'Deploying...'
             }
         }
     }
 }
